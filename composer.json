{
    "name": "satyam147/laravel-zoom",
    "description": "Laravel Zoom package",
    "homepage": "https://github.com/satyam147/laravel-zoom",
    "keywords": [
        "macsidigital",
        "satyam147",
        "laravel-zoom",
        "zoom",
        "laravel",
        "api",
        "client"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Colin Hall",
            "email": "colin@macsi.co.uk",
            "role": "Developer"
        },
        {
            "name": "Satyam Dasari",
            "email": "satyam161211149@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "satyam147/laravel-api-client": "^6.1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16|^3.14",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "MacsiDigital\\Zoom\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MacsiDigital\\Zoom\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MacsiDigital\\Zoom\\Providers\\ZoomServiceProvider"
            ],
            "aliases": {
                "Zoom": "MacsiDigital\\Zoom\\Facades\\Zoom"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
